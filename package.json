{
    "name": "start",
    "version": "0.0.1",
    "description": "A way to start web applications",
    "private": true,
    "license": "MIT",
    "author": "@davi-mbatista",
    "scripts": {
        "start": "webpack-dev-server --config webpack/start.js",
        "build": "webpack --config webpack/build.js"
    },
    "devDependencies": {
        "@babel/cli": "7.10.5",
        "@babel/core": "7.10.5",
        "@babel/preset-env": "7.10.4",
        "babel-eslint": "10.1.0",
        "babel-loader": "8.1.0",
        "clean-webpack-plugin": "3.0.0",
        "copy-webpack-plugin": "6.0.3",
        "css-loader": "4.1.1",
        "cssnano": "4.1.10",
        "eslint": "7.5.0",
        "eslint-loader": "4.0.2",
        "file-loader": "6.0.0",
        "html-webpack-plugin": "4.3.0",
        "mini-css-extract-plugin": "0.9.0",
        "postcss-loader": "3.0.0",
        "postcss-preset-env": "6.7.0",
        "prettier": "2.0.5",
        "script-ext-html-webpack-plugin": "2.1.4",
        "size-plugin": "2.0.1",
        "style-loader": "1.2.1",
        "svelte-loader": "2.13.6",
        "webpack": "4.44.0",
        "webpack-cli": "3.3.12",
        "webpack-dev-server": "3.11.0",
        "webpack-merge": "5.0.9",
        "optimize-css-assets-webpack-plugin": "5.0.3"
    },
    "dependencies": {
        "svelte": "3.24.0"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "browserslist": [
        "last 3 chrome version",
        "last 3 firefox version",
        "last 3 safari version",
        "last 3 ios_saf version"
    ],
    "postcss": {
        "plugins": {
            "postcss-preset-env": {},
            "cssnano": {}
        }
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "env": {
            "browser": true,
            "node": true,
            "es6": true,
            "worker": true,
            "serviceworker": true
        },
        "extends": [
            "eslint:recommended"
        ],
        "rules": {
            "no-console": "off"
        }
    },
    "eslintIgnore": [
        "build",
        "node_modules"
    ],
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "none",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "avoid"
    }
}
