{
    "name": "start",
    "version": "0.0.1",
    "description": "A way to start web applications",
    "private": true,
    "license": "MIT",
    "author": "@davi-mbatista",
    "scripts": {
        "start": "webpack serve --config webpack/start.js",
        "build": "webpack --config webpack/build.js"
    },
    "devDependencies": {
        "@babel/cli": "7.12.10",
        "@babel/core": "7.12.10",
        "@babel/preset-env": "7.12.11",
        "@babel/preset-react": "7.12.10",
        "babel-eslint": "10.1.0",
        "babel-loader": "8.2.2",
        "clean-webpack-plugin": "3.0.0",
        "copy-webpack-plugin": "7.0.0",
        "css-loader": "5.0.1",
        "cssnano": "4.1.10",
        "eslint": "7.15.0",
        "eslint-loader": "4.0.2",
        "eslint-plugin-react": "7.21.5",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "4.5.0",
        "mini-css-extract-plugin": "1.3.3",
        "optimize-css-assets-webpack-plugin": "5.0.4",
        "postcss-loader": "4.1.0",
        "postcss-preset-env": "6.7.0",
        "prettier": "2.2.1",
        "script-ext-html-webpack-plugin": "2.1.5",
        "size-plugin": "2.0.1",
        "style-loader": "2.0.0",
        "webpack": "5.10.3",
        "webpack-cli": "4.2.0",
        "webpack-dev-server": "3.11.0",
        "webpack-merge": "5.7.0"
    },
    "dependencies": {
        "preact": "10.5.7"
    },
    "babel": {
        "presets": [
            "@babel/env",
            [
                "@babel/preset-react",
                {
                    "pragma": "h"
                }
            ]
        ]
    },
    "browserslist": [
        "last 3 chrome version",
        "last 3 firefox version",
        "last 3 safari version",
        "last 3 ios_saf version"
    ],
    "postcss": {
        "plugins": {
            "postcss-preset-env": {},
            "cssnano": {}
        }
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "env": {
            "browser": true,
            "node": true,
            "es6": true,
            "worker": true,
            "serviceworker": true
        },
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended"
        ],
        "settings": {
            "react": {
                "pragma": "h",
                "version": "latest"
            }
        },
        "rules": {
            "no-console": "off"
        }
    },
    "eslintIgnore": [
        "build",
        "node_modules"
    ],
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "none",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "avoid"
    }
}
